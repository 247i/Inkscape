[run]
omit = tests/*
       inkex/deprecated-simple/*
       inkex/deprecated.py
       .tox/*
       index.py
       setup.py
       inkex.py
       other/*

[report]
exclude_lines =
    # Don't complain if non-runnable code isn't run:
    pragma: no cover
    if __name__ == .__main__.:
    @overload

[paths]
source =
    ./
    C:\GitLab-Runner\builds\*\extensions
    C:\GitLab-Runner\builds\*\*\extensions
    C:\builds\*\extensions
    /builds/*/extensions
