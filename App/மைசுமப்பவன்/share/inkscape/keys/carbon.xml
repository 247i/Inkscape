  <bind gaction="win.dialog-open('FillStroke')"          keys="&lt;primary&gt;&lt;shift&gt;f, &lt;primary&gt;&lt;shift&gt;F" />
  <bind gaction="win.dialog-open('FilterEffects')" />
  <bind gaction="win.dialog-open('Find')"                keys="&lt;primary&gt;f, &lt;primary&gt;F" />
  <bind gaction="win.dialog-open('Glyphs')" />
  <bind gaction="win.dialog-open('IconPreview')" />
  <bind gaction="win.dialog-open('Input')" />
  <bind gaction="win.dialog-open('LivePathEffect')"      keys="&lt;primary&gt;&lt;shift&gt;ampersand, &lt;primary&gt;&lt;shift&gt;7" />
