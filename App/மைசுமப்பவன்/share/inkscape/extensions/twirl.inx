<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <name>Twirl</name>
    <id>org.ekips.filter.twirl</id>
    <param name="twirl" type="float" min="0.00" max="1000.00" gui-text="Amount of twirl:">5.0</param>
    <param name="rotation" type="bool" gui-text="Rotation is clockwise">true</param>
    <effect>
        <object-type>path</object-type>
        <effects-menu>
            <submenu name="Modify Path"/>
        </effects-menu>
    </effect>
    <script>
        <command location="inx" interpreter="python">twirl.py</command>
    </script>
</inkscape-extension>
